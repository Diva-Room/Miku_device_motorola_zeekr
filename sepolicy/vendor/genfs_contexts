# Power supply device nodes
genfscon sysfs /devices/platform/soc/988000.i2c/i2c-1/1-0064/power_supply/flip_battery             u:object_r:vendor_sysfs_battery_supply:s0
genfscon sysfs /devices/platform/soc/988000.i2c/i2c-1/1-006b/power_supply/charger                  u:object_r:vendor_sysfs_battery_supply:s0

# Wakeup
genfscon sysfs /devices/platform/soc/988000.i2c/i2c-1/1-0064/power_supply/flip_battery/wakeup19    u:object_r:vendor_sysfs_battery_supply:s0
genfscon sysfs /devices/platform/soc/988000.i2c/i2c-1/1-006b/power_supply/charger/wakeup35         u:object_r:sysfs_wakeup:s0
